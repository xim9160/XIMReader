  pod 'Fuzi'

  # Cache
  pod 'SQLite.swift'
  pod 'FMDB'
  
  # route
  pod 'URLNavigator'

  
  # szfy
  pod 'AFNetworking'
  pod 'SDWebImage', '~> 4.0'
  pod 'SDWebImage/GIF'
  pod 'FLAnimatedImage'
  pod 'SVProgressHUD'
  pod 'MBProgressHUD'

  # MOB主模块(必须)
  pod 'mob_smssdk'
  pod 'mobAPI'
  pod "AGEmojiKeyboard"
  pod 'JSONModel'
  pod 'YYModel'
  pod 'MJExtension'
  pod 'ReactiveObjC', '~> 2.1.0'
  pod 'Masonry'
