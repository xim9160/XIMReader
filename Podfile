# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'XIMReader' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for XIMReader

  # Networking
  pod 'Alamofire'

  # Resource
  pod 'Kingfisher'

  # UI
  pod 'SnapKit'
  pod 'Then'
  pod 'MJRefresh'

  # XML/HTML Parser
  pod 'Fuzi'

  # Cache
  pod 'SQLite.swift'
  pod 'FMDB'
  
  # route
  pod 'URLNavigator'

  target 'XIMReaderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'XIMReaderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
