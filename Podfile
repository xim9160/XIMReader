  pod 'TYAttributedLabel', '~> 2.6.2'
  pod 'YYText'
  pod 'TTTAttributedLabel'
  pod 'YYModel'
  pod 'XXNibBridge', '~> 2.3.0'
  pod 'YBPopupMenu', '~> 1.1.2'
  
  # 组件化工程
  pod 'SZFYDependciesLib', '~> 0.1.4'
  
  
  target 'XIMReaderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'XIMReaderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

